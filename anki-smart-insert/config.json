{
    "bullets":["•"],
    "options":{
        "output": "‒",
        "include_whitespaced": true,
        "add_whitespace": true
    },
    "delete_newlines":{
        "after_symbol": true,
        "in_text": true
    },
    "additional_replacements":[
        {
            "input":[],
            "output": ""
        }
    ],
    "numbered_lists": {
        "activate": true,
        "filter_regex": "^((\\d[\\.\\)]?)+)(.*)",
        "enumeration_reference": "\\g<1>",
        "content_reference": "\\g<3>"
    },
    "ai":{
        "enabled": true,
        "front_regex": "^[FfQq]: *(.*)$",
        "front_mask": "\\g<1>",
        "back_regex": "^A: *(.*)$",
        "back_mask": "\\g<1>",
        "tag_generated_cards": true
    },
    "debug_mode": false
}